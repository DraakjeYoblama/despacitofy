{
  "manifest_version": 2,

  "name": "Squanchify",
  "description": "This extension makes your browsing experience...Squanchier.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "howSquanchy.html"
  },
  "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "css": ["squanchyStyle.css"],
          "js": ["jquery.js", "squanchify.js"]
      }
  ],
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
